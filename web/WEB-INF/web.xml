<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:chemaLocation="http://java.sun.com/xml/ns/javaee
         http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
    <servlet>
        <servlet-name>Start</servlet-name>
        <servlet-class>CowBull.servlets.Start</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Start</servlet-name>
        <url-pattern></url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Game</servlet-name>
        <servlet-class>CowBull.servlets.Game</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Game</servlet-name>
        <url-pattern>/Game</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Dispetcher</servlet-name>
        <servlet-class>CowBull.servlets.Dispetcher</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Dispetcher</servlet-name>
        <url-pattern>/Dispetcher</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Registration</servlet-name>
        <servlet-class>CowBull.servlets.Registration</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Registration</servlet-name>
        <url-pattern>/Reg</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Process</servlet-name>
        <servlet-class>CowBull.servlets.Process</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Process</servlet-name>
        <url-pattern>/Process</url-pattern>
    </servlet-mapping>

</web-app>